<div class="header clearfix">
  <nav>
    <ul class="nav nav-pills pull-right">
      <form method="get" action="/user/manageUsers">
        <button type="submit" class="btn btn-default">Manage Users</button>
      </form>
    </ul>
  </nav>
</div>

<h4 class="text-muted">{{formName}}</h4>
<form method="get" action="/form/addData">
  <button type="submit" class="btn btn-default">Add</button>
</form> <br/>
<div>
    <table class="table table-bordered">
      <thead>
        <th/>
        {{#each formHeaders}}
          <th>{{name}}</th> 
        {{/each}}
      </thead> 
      <tbody> 
          {{#each formData}}
            <tr>

              {{#each this}}
                {{#if this._id}}
                  <td width="50">
                      <button type="submit" class="glyphicon glyphicon-pencil btn btn-link"/>
                  </td>
                {{/if}}
              {{/each}}

              {{#each this}}
                {{#if this.value}}
                  <td>{{this.value}}</td>
                {{/if}}
              {{/each}}

            </tr>
          {{/each}} 
      </tbody>
  </table>
</div>